{
    "version": 2,
    "builds": [
      {
        "src": "src/server.ts", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/server.ts"
      }
    ],
    "ignoreCommand": "npm run test",
    "devCommand": "npm run dev",
    "buildCommand": "npm run build",
    "startCommand": "npm run start"
  }
  